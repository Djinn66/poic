{% extends 'base.html.twig' %}

{% block title %}Inaptitude{% endblock %}

{% block body %}
    <h1>Inaptitude</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inaptitude.id }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ inaptitude.dateDebut ? inaptitude.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ inaptitude.dateFin ? inaptitude.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fichier</th>
                <td>{{ inaptitude.fichier }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('inaptitude_index') }}">back to list</a>

    <a href="{{ path('inaptitude_edit', {'id': inaptitude.id}) }}">edit</a>

    {{ include('inaptitude/_delete_form.html.twig') }}
{% endblock %}
